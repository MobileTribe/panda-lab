{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "es2015"
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*",
    "dist"
  ]
}
