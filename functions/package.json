{
  "name": "functions",
  "scripts": {
    "preinstall": "if [ -d ../commons ]; then cd ../commons && npm install && cd ../functions && npm run updateCommons; fi",
    "updateCommons": "node commons-install.js",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "serve": "npm run build && npm run setup-env firebase serve --only functions",
    "emulators": "npm run build && npm run setup-env firebase emulators:start",
    "shell": "npm run build && npm run setup-env firebase functions:shell",
    "start": "npm run shell",
    "setup-env": "GOOGLE_APPLICATION_CREDENTIALS='../.config/firebase-adminsdk.json'",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/firestore": "^2.2.7",
    "adbkit-apkreader": "^3.1.2",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^6.4.0",
    "firebase-admin": "^8.2.0",
    "firebase-functions": "^3.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "pandalab-commons": "file:packages/pandalab-commons-0.0.1.tgz",
    "uuid": "^3.3.2",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/multer": "^1.3.7",
    "@types/yauzl": "^2.9.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "private": true,
  "engines": {
    "node": "8"
  }
}
